{{ form(form) }}

{% for message in messages %}
    <br>
    <h2>{{ message.userid.avatar }}</h2>
    <h2>{{ message.userid.pseudo }}</h2>
    <h3> {{ message.message }} </h3> 
   <p> likes : {{ message.Aime | length }} </p>
    <a href="{{ path('Aime', {message: message.id}) }}">Like</a>
    <a href="{{ path('private_message', {destinate_id: message.userid.id}) }}">private</a>
{% endfor %}

{{ dump(messages) }}


